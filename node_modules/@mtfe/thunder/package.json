  "scripts": {
    "build": "node build",
    "inspect": "cross-env NODE_ENV=development && node --inspect ./node_modules/webpack-dev-server/bin/webpack-dev-server --config ./examples/dev/build/webpack.conf.js --hot",
    "lint": "npm run lint:client && npm run lint:sw && npm run lint:plugin && npm run lint:server",
    "lint:client": "eslint --ext .js 'lib/client/*'",
    "lint:plugin": "eslint --ext .js 'plugin/*'",
    "lint:server": "eslint --ext .js 'server/*'",
    "lint:sw": "eslint --ext .js 'lib/service-worker-runtime/*'",
    "prepublishOnly": "npm run lint && npm run test && npm run build",
    "start:app": "node examples/dev/app",
    "start:app:dev": "node --inspect examples/dev/app",
    "start:build": "cd examples/dev/ && cross-env NODE_ENV=production webpack --config ./build/webpack.conf.js",
    "start:dev": "cd examples/dev/ && cross-env NODE_ENV=development node --inspect ./node_modules/.bin/webpack-dev-server --config ./build/webpack.conf.js  --host 0.0.0.0 --hot",
    "start:upload": "stark-uploader --appkey 3d07127a --dir-mode examples/dev/static/offlinify/simple",
    "test": "npm run test:loader && npm run test:plugin && npm run test:server && npm run test:sw",
    "test:client": "karma start test/client/karma.conf.js",
    "test:debug": "cross-env DEBUG=true karma start test/client/karma.conf.js",
    "test:idb": "NODE_ENV=testing nyc mocha --recursive -r @std/esm test/loader/indexed-spec.js",
    "test:loader": "NODE_ENV=testing nyc mocha --recursive -r @std/esm test/loader/*-spec.js",
    "test:plugin": "npm run test:plugin:w3 && npm run test:plugin:w4",
    "test:plugin:w3": "mocha --recursive test/plugin/webpack3/*-spec.js",
    "test:plugin:w4": "mocha --recursive test/plugin/webpack4/*-spec.js",
    "test:server": "mocha --recursive test/server/*-spec.js",
    "test:sw": "mocha --check-leaks --globals location -r @std/esm --recursive test/service-worker/*-spec.js"
  },
